;;; package --- Lua configurations

;;; Commentary:
;; - Lua configurations
;; - Lua-mode



;;; Code:
(use-package lua-mode
  :ensure t
  :config
  (autoload 'lua-mode "lua-mode" "Lua editing mode." t)
  (add-to-list 'auto-mode-alist '("\\.lua$" . lua-mode))
  (add-to-list 'interpreter-mode-alist '("lua" . lua-mode)))


(provide 'luaLayer)
;;; luaLayer.el ends here
