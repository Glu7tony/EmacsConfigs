      <script type="text/javascript">
       function changeEnableSalary ()
					{
						if ($ ('#enable_salary').is (":checked")) {
							$ ('#salaryContainer').show ();
							$ ('#salary_from_1').prop ('checked', true);
							$ ('#salary_tr').show ();
							$ ('#salary_arr').removeClass ('arr_d').addClass ('arr_up');
							$ ('#sel_salary').hide ();
						}
						else {
							$ ('#salaryContainer').hide ();
							var radio = $ ('input[name="salary_from"]:checked');
							radio.prop ('checked', false);
						}
					}
      </script>
