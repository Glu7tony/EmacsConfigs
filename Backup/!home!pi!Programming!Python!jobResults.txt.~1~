        <script type="text/javascript">
         var quickLinksCallback = null;

    function showQuickLinksPopup (callbackFn)
    {
        quickLinksCallback = callbackFn;
        createOverlay ();
        openAlertPopup ($ ('#quicklinks_popup'));
    }

    function quickLinksConfirm (confirm, quickLinksConfirmCallback)
    {
        $.ajax ({
            type: "POST",
            url: "quicklinks_consent.php",
            data: {'confirm': confirm},
            success: function (html) {

                if (quickLinksConfirmCallback) {
                    quickLinksConfirmCallback ();
                }
                else if (quickLinksCallback) {
                    quickLinksCallback ();
                }
            }
        });
    }
        </script>
        <div class="ajax-alert-container" id="quicklinks_popup" style="display:none;  width:650px; margin-left:-325px">
         <a href="javascript:;" onclick="$('#quicklinks_popup').hide(); clearOverlay();" style="z-index:12;display: block;float: right; position: relative; margin: -10px -10px 0px 0px;">
          <img border="0" src="images/popup_close_ico.gif"/>
         </a>
