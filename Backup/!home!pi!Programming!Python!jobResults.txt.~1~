       function closeCookieBar () {
						 var d = new Date();
						 d.setTime(d.getTime() + (180*24*60*60*1000));
						 document.cookie = "HIDE_COOKIES_INFOBOX=1; expires=" + d.toUTCString();
						 document.getElementById('cookie_bar').style.display = 'none';
					}
      </script>
